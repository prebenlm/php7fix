#!/bin/bash
# siste fra simeng: sed 's/\(->\)\($[^)]+\)/\1{\2}/'
# virket ganske bra: s/\(->\)\(\$[^(]\+\)/\1{\2}/
# enda ny: 's/\(->\)\($[a-Z0-9_ $\[\]]\+\)(/\1{\2}/'
# s/\(->\)\($[][$a-z0-9_ ]\+\)\((\)/\1{\2}\3/
sed 's/\(->\)\($[][$a-z0-9_ ]\+\)\((\)/\1{\2}\3/' < "$1" > "$1.tmp" && mv "$1.tmp" "$1" && git diff "$1" && git checkout "$1"